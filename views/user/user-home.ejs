<body>
    <h1>MarktPlatz</h1>
    <br><br>

    <ul>
      <li><a href="/users"><%= user.first_name %>  <%= user.last_name %></a> </li>
      <li>
        <select>
            <option selected>--- Settings ---</option>
            <option><a href="/users/edit-profile">Edit Profile</a></option>
            <option><a href="/users/change-password">Change Password</a></option>
            <option><a href="/users/logout">Log Out</a></option>
        </select>
      </li>

    </ul>

    <ul>
      <li><a href="/about_us">About Us</a></li>
      <li> <a href="/list_companies">List Companies</a> </li>
    </ul>


    <h4>---------------------------------------------------------------------------------------------------------------------------------------</h4>
    <h3>Your Profile</h3>
    <table style="text-align: left">
      <tr>
        <th>Name:</th>
        <td><%= user.first_name %>  <%= user.last_name %></td>
      </tr>
      <tr>
        <th>Address:</th>
        <td><%= user.address %></td>
      </tr>
      <tr>
          <th>Birth Date:</th>
          <td><%= user.birthdate %></td>
      </tr>
      <tr>
        <th>City:</th>
        <td><%= user.city %></td>
      </tr>
      <tr>
          <th>Province:</th>
          <td><%= user.province %></td>
      </tr>
      <tr>
          <th>Email:</th>
          <td><%= user.email %></td>
      </tr>
    </table>
    
