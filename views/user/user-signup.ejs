    <h3>Register User </h3>

    <br><br>
    <form action="/user/register" method="POST">
      First Name:  <br>
      <input type="text" name="first_name" placeholder="First Name" value="<%= dataUser.first_name%>"> 
      <br><br>
      Last Name:  <br>
      <input type="text" name="last_name" placeholder="Last Name" value="<%= dataUser.last_name%>">
      <br><br>
      Birthdate: <br>
      <input type="date" name="birthdate" placeholder="Birthdate" value="<%= dataUser.birthdate%>">
      <br><br>
      Address:  <br>
      <input type="text" name="address" placeholder="Address" value="<%= dataUser.address%>">
      <br><br>
      City:  <br>
      <input type="text" name="city" placeholder="City" value="<%= dataUser.city%>">
      <br><br>
      Province: <br>
      <input type="text" name="province" placeholder="Province" value="<%= dataUser.province%>">
      <br><br>
      Email:  <br>
      <input type="text" name="email" placeholder="Email" value="<%= dataUser.email%>">
      <br><br>
      Password:  <br>
      <input type="password" name="password" placeholder="password" value="<%= dataUser.password%>">
      <br><br>
      <button type="submit">submit data</button>
    </form>


  </body>

</html>